{
  "name": "tourze/cloudflare-dns-bundle",
  "description": "Cloudflare DNS Manage",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/persistence": "^3.1 || ^4",
    "doctrine/orm": "^3.0",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/http-client": "^6.4",
    "symfony/console": "^6.4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-client-contracts": "^2.5 | ^3.0",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/enum-extra": "~0.0.5",
    "tourze/easy-admin-attribute": "0.1.*",
    "tourze/doctrine-track-bundle": "0.1.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "psr/log": "^3|^2|^1"
  },
  "autoload": {
    "psr-4": {
      "CloudflareDnsBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CloudflareDnsBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  }
}